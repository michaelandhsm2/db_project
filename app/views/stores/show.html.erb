<h1>Stores#show</h1>
<p>Find me in app/views/stores/show.html.erb</p>

<div class = "item"><%= @store.id %></div>
<div class = "item"><%= @store.name %></div>
<div class = "item"><%= @store.email %></div>
<div class = "item"><%= @store.phone %></div>
<div class = "item"><%= @store.description %></div>
<div><%= image_tag(@store.image.url, alt: @store.name) %></div>

<%= render 'items/list', items: @store.items.paginate(page: params[:page], :per_page => 5) %>
